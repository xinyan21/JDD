            name="MODE_TACTICS"
            description="配置线程池的模式[THREAD_SINGLE | THREAD_MULTI]"
            value="THREAD_MULTI" />
        <option
            name="MAX_POOL_SIZE"
            description="配置线程池的最大活动数"
            value="10" />
