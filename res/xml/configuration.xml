    <!-- http配置 -->
    <http>
        <parameter
            name="REQUEST_RETRY_COUNT"
            description="设置连接重置或超时后的请求自动重试次数,默认为0"
            value="3" />
        <parameter
            name="MAX_CONNECTION"
            description="最大连接数"
            value="100" />
        <parameter
            name="WAIT_TIMEOUT"
            description="连接的最大等待时间"
            value="60000" />
        <parameter
            name="CONNECTION_TIMEOUT"
            description="连接超时"
            value="30000" />
        <parameter
            name="REQUEST_TIMEOUT"
            description="请求超时"
            value="30000" />
        <parameter
            name="MAX_LINK_ROUTE"
            description="设置每个路由最大连接数,设置目标机的连接访问的路由连接数,默认是2条"
            value="100" />
    </http>

    <!-- 线程池的配置 -->
    <threadpool>
